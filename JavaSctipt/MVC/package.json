{
  "name": "static-html-webpack-boilerplate",
  "version": "1.1.1",
  "description": "private blog",
  "private": true,
  "main": "index.js",
  "scripts": {
    "build": "webpack --config webpack.config.prod.js",
    "lint:js": "./node_modules/.bin/eslint src/js/*.js",
